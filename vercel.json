{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "version": 2,
  "name": "cambrian-api",
  "public": false,
  "github": {
    "enabled": true,
    "autoAlias": true,
    "silent": false
  },
  "buildCommand": "dotnet publish src/Cambrian.Api/Cambrian.Api.csproj -c Release -o out",
  "outputDirectory": "out",
  "installCommand": "echo 'No package manager installation needed for .NET'",
  "framework": null,
  "env": {
    "ASPNETCORE_ENVIRONMENT": "Production"
  }
}
